#include <iostream>
#include <stack>
#include <string>
using namespace std;

void deleteMiddle(stack<int>& s, int count, int size){
    if(count == size/2){
        s.pop();
        return;
    }
    int num = s.top();
    s.pop();
    
    deleteMiddle(s,count + 1,size);
    
    s.push(num);
}
int main(){
    stack<int> s;
    
    s.push(12);
    s.push(10);
    s.push(33);
    s.push(9);
    s.push(14);
    
    int size = s.size();
    
    deleteMiddle(s, 0 , size);
    
    cout << "Stack after deleting the middle element:\n";
    while(!s.empty()){
        cout << s.top() << " ";
        s.pop();
        
    }
    return 0;
}
