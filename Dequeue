#include <iostream>
#include <deque>
using namespace std;

int main() {
    deque<int> dq;

    dq.push_back(10);  // insert at rear
    dq.push_front(20); // insert at front

    cout << "Front: " << dq.front() << " , Back: " << dq.back() << endl;

    dq.pop_front();
    dq.pop_back();

    return 0;
}
