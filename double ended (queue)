#include<iostream>
#include<deque>
using namespace std;

int main(){
    deque<int> d;
    
    d.push_front(12);
    d.push_back(14);
    
    cout << "Front: " << d.front() << endl;
    cout << "Back: " << d.back() << endl;
    
    d.pop_front();
    d.pop_back();
    
    if(d.empty()) cout << "Deque is empty\n";
    else cout << "Deque is not empty\n";
    
    return 0;
}
