#include <stack>
#include <iostream>
using namespace std;

int main() {
    stack<int> s;     

    s.push(2);        
    s.push(3);       
    s.pop();        

    cout << "Top: " << s.top() << endl; 
    cout << (s.empty() ? "Empty" : "Not empty") << endl;
    cout << "Size: " << s.size() << endl;

    return 0;
}
